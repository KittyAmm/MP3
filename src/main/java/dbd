CREATE SEQUENCE idalbums START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE idutilisateur START WITH 1 INCREMENT BY 1;

select count(id) as nb from Telechargement where iduser ='6'

CREATE TABLE Utilisateur(
  id VARCHAR (10) PRIMARY KEY,
  pseudo VARCHAR (50),
  password VARCHAR(50),
  email VARCHAR(100),
  birth DATE,
  sexe VARCHAR(10)
);

CREATE TABLE Artiste(
  id VARCHAR (10),
  nomartiste VARCHAR(100),
  image VARCHAR(10)
);

CREATE TABLE Albums(
  id VARCHAR (10),
  idart VARCHAR (10),
  nomalbum VARCHAR (100),
  image VARCHAR(10)
);

CREATE TABLE Genre(
  id VARCHAR (10),
  type VARCHAR (50),
  image VARCHAR(10)
);

CREATE TABLE Chanson(
  id VARCHAR(10) PRIMARY KEY ,
 titre VARCHAR(500),
 duree int,
 favoris int,
 idutilisateur VARCHAR(10),
  idartiste VARCHAR(10),
  idalbum VARCHAR(10),
  idgenre VARCHAR(10),
  dateajout DATE,
  FOREIGN KEY(idutilisateur) REFERENCES Utilisateur(id),
  FOREIGN KEY(idartiste) REFERENCES Artiste(id),
  FOREIGN KEY(idalbum) REFERENCES Albums(id),
  FOREIGN KEY(idgenre) REFERENCES Genre(id)
  );

CREATE TABLE PlayList(
    id VARCHAR (10) PRIMARY KEY ,
    datedajout VARCHAR (50),
    idson VARCHAR(20),
  iduser VARCHAR(20),
  titre VARCHAR(100),
  FOREIGN KEY (idson) REFERENCES mp3info(id),
  FOREIGN KEY (iduser) REFERENCES Utilisateur(id)
);
create TABLE Menu(
  id VARCHAR(10) PRIMARY KEY ,
)
select * from Mp3Info offset 0 fetch next 5 rows only
