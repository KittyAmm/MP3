CREATE TABLE Utilisateur(
  id VARCHAR (10) PRIMARY KEY,
  username VARCHAR (50),
  password VARCHAR(50),
  email VARCHAR(100),
  birth DATE,
  sexe VARCHAR(10)
);

CREATE TABLE Artiste(
  id VARCHAR (10),
  nomartiste VARCHAR(100),
  image VARCHAR(10)
);

CREATE TABLE Albums(
  id VARCHAR (10),
  idart VARCHAR (10),
  nomalbum VARCHAR (100),
  image VARCHAR(10)
);

CREATE TABLE Genre(
  id VARCHAR (10),
  type VARCHAR (50),
  image VARCHAR(10)
);

CREATE TABLE Chanson(
  id VARCHAR(10) PRIMARY KEY ,
 titre VARCHAR(500),
 duree int,
 favoris int,
 idutilisateur VARCHAR(10),
  image VARCHAR(20),
  idartiste VARCHAR(10),
  idalbum VARCHAR(10),
  idgenre VARCHAR(10),
  dateajout DATE,
  FOREIGN KEY(idutilisateur) REFERENCES Utilisateur(id),
  FOREIGN KEY(idartiste) REFERENCES Artiste(id),
  FOREIGN KEY(idalbum) REFERENCES Albums(id),
  FOREIGN KEY(idgenre) REFERENCES Genre(id)
  );

CREATE TABLE PlayList(
    id VARCHAR (10) PRIMARY KEY ,
    nom VARCHAR (50),
    description VARCHAR (200),
    image VARCHAR (200),
    idchanson VARCHAR(10),
  FOREIGN KEY (idchanson) REFERENCES Chanson(id)
);
create TABLE Menu(
);